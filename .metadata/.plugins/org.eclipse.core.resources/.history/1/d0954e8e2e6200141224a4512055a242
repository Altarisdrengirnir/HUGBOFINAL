package Altaris;

public class TicTacToe
{
	private final int GRIDLINE = 3;
	public char grid[];
	public char turn;
	//Initial empty grid.
	public TicTacToe()
	{
		//           012345678
		this.grid = "---------".toCharArray();
		this.turn = 'X';
	}
	
	public String toString()
	{
		return new String(grid);
	}
	

}
